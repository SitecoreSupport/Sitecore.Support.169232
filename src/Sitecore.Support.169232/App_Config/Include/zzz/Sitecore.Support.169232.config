<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/">
  <sitecore>
    <social>
      <networks>
        <network name="Twitter">
          <providers>
            <provider type="Sitecore.Social.Twitter.Networks.Providers.TwitterProvider, Sitecore.Social.Twitter" set:type="Sitecore.Support.Social.Twitter.Networks.Providers.TwitterProvider, Sitecore.Support.169232"/>
          </providers>
        </network>
      </networks>
      <profileKeyMappings>
        <network name="Twitter">
          <field enabled="true" originalKey="Email" sitecoreKey="tw_Email" text="Email" />
        </network>
      </profileKeyMappings>
    </social>
  </sitecore>
</configuration>